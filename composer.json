{
  "name": "bookshelves/back",
  "type": "project",
  "description": "Back of Bookshelves application.",
  "license": "BSD 2-Clause",
  "version": "1.0.0",
  "homepage": "https://github.com/bookshelves-project",
  "authors": [
    {
      "name": "ewilan-riviere",
      "email": "ewilan.riviere@gmail.com",
      "role": "Developer"
    },
    {
      "name": "Mopi",
      "email": "mopi@gdotslashplay.it",
      "role": "Tester, ideas"
    }
  ],
  "keywords": [
    "api",
    "epubs",
    "books",
    "ebooks"
  ],
  "suggest": {
    "ext-imagick": "*",
    "ext-rar": "*"
  },
  "require": {
    "php": "^8.2.0",
    "artesaos/seotools": "^1.0.0",
    "filament/filament": "^2.0",
    "filament/notifications": "^2.0",
    "filament/spatie-laravel-media-library-plugin": "^2.0",
    "filament/spatie-laravel-settings-plugin": "^2.0",
    "filament/spatie-laravel-tags-plugin": "^2.0",
    "filament/spatie-laravel-translatable-plugin": "^2.0",
    "guzzlehttp/guzzle": "^7.2",
    "http-interop/http-factory-guzzle": "^1.2",
    "itsgoingd/clockwork": "^5.1",
    "kiwilan/laravel-steward": "dev-main",
    "laravel/framework": "^9.19",
    "laravel/sanctum": "^3.0",
    "laravel/scout": "^9.4",
    "laravel/tinker": "^2.7",
    "livewire/livewire": "^2.10",
    "meilisearch/meilisearch-php": "^0.25",
    "opcodesio/log-viewer": "^1.4",
    "pxlrbt/filament-excel": "^1.1",
    "smalot/pdfparser": "^2.2",
    "spatie/array-to-xml": "^3.1",
    "spatie/image": "^2.2",
    "spatie/laravel-medialibrary": "^10.0.0",
    "spatie/laravel-query-builder": "^5.0",
    "spatie/laravel-ray": "^1.31",
    "spatie/laravel-route-attributes": "^1.12",
    "spatie/laravel-settings": "^2.4",
    "spatie/laravel-tags": "^4.3",
    "spatie/laravel-translatable": "^6.0",
    "stechstudio/filament-impersonate": "^2.5"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.12",
    "beyondcode/laravel-dump-server": "^1.8",
    "fakerphp/faker": "^1.9.1",
    "friendsofphp/php-cs-fixer": "^3.9",
    "fruitcake/laravel-telescope-toolbar": "^1.3",
    "knuckleswtf/scribe": "^4.0.0",
    "laravel/pint": "^1.0",
    "laravel/sail": "^1.0.1",
    "laravel/telescope": "^4.9",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^6.1",
    "nunomaduro/larastan": "^2.1",
    "pestphp/pest": "^1.21",
    "pestphp/pest-plugin-laravel": "^1.2",
    "phpunit/phpunit": "^9.5.10",
    "rakutentech/laravel-request-docs": "^1.26",
    "spatie/laravel-ignition": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
      "@php artisan optimize:clear",
      "@php artisan ide-helper:generate",
      "@php artisan ide-helper:models --nowrite --reset",
      "php artisan ide-helper:meta",
      "@php artisan ide-helper:eloquent",
      "@php artisan filament:upgrade"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "format": [
      "php-cs-fixer fix --ansi"
    ],
    "helper": [
      "@php artisan ide-helper:generate",
      "@php artisan ide-helper:models --nowrite --reset",
      "php artisan ide-helper:meta",
      "@php artisan ide-helper:eloquent"
    ],
    "analyse": [
      "phpstan analyse --ansi --memory-limit=4G"
    ],
    "docs": [
      "php artisan scribe:generate"
    ],
    "test": [
      "@php artisan test"
    ],
    "test:watch": [
      "Composer\\Config::disableProcessTimeout",
      "phpunit-watcher watch < /dev/tty"
    ],
    "queue": [
      "Composer\\Config::disableProcessTimeout",
      "php artisan queue:listen"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "laravel/telescope"
      ]
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "phpro/grumphp-shim": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}

{
    "name": "bookshelves/back",
    "type": "project",
    "description": "Back of Bookshelves application.",
    "license": "BSD 2-Clause",
    "version": "1.0.0",
    "homepage": "https://gitlab.com/bookshelves-project/bookshelves-back",
    "authors": [
        {
            "name": "ewilan-riviere",
            "email": "ewilan.riviere@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Mopi",
            "email": "mopi@gdotslashplay.it",
            "role": "Tester, ideas"
        },
        {
            "name": "adr1enbe4udou1n",
            "email": "adrien.beaudouin@outlook.com",
            "role": "Creator of rad stack"
        }
    ],
    "keywords": [
        "api",
        "epubs",
        "books",
        "ebooks"
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://gitlab.com/bookshelves-project/bookshelves-back"
        }
    ],
    "require": {
        "php": "^8.0",
        "fluentdom/fluentdom": "^8.0",
        "fruitcake/laravel-cors": "^2.0.5",
        "genl/matice": "^1.1",
        "guzzlehttp/guzzle": "^7.2",
        "http-interop/http-factory-guzzle": "^1.2",
        "inertiajs/inertia-laravel": "^0.5.4",
        "itsgoingd/clockwork": "^5.1",
        "laravel/fortify": "^1.10",
        "laravel/framework": "^9.0",
        "laravel/sanctum": "^2.14",
        "laravel/scout": "^9.4",
        "laravel/tinker": "^2.7",
        "league/commonmark": "^2.2",
        "league/glide-symfony": "^2.0",
        "league/html-to-markdown": "^5.0",
        "livewire/livewire": "^2.10",
        "maatwebsite/excel": "^3.1",
        "meilisearch/meilisearch-php": "^0.21.0",
        "oscarotero/inline-svg": "^2.0",
        "spatie/array-to-xml": "^3.1",
        "spatie/eloquent-sortable": "^4.0",
        "spatie/laravel-enum": "^3.0",
        "spatie/laravel-image-optimizer": "^1.7",
        "spatie/laravel-medialibrary": "^10.0.4",
        "spatie/laravel-query-builder": "^5.0",
        "spatie/laravel-route-attributes": "^1.9",
        "spatie/laravel-sluggable": "^3.3",
        "spatie/laravel-tags": "^4.3",
        "spatie/laravel-translatable": "^5.2",
        "tightenco/ziggy": "1.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.6",
        "knuckleswtf/scribe": "^3.23",
        "laravel/sail": "^1.0.1",
        "laravel/telescope": "^4.7",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/larastan": "^2.0",
        "pestphp/pest-plugin-laravel": "^1.2",
        "phpro/grumphp-shim": "^1.8",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:models --nowrite --reset",
            "@php artisan ide-helper:eloquent"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "helper": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:models --nowrite --reset",
            "@php artisan ide-helper:eloquent"
        ],
        "doc": [
            "@php artisan route:clear",
            "@php artisan scribe:generate --force"
        ],
        "format": [
            "php-cs-fixer fix --ansi"
        ],
        "analyse": [
            "phpstan analyse --ansi"
        ],
        "test": [
            "@php artisan test"
        ],
        "test:watch": [
            "Composer\\Config::disableProcessTimeout",
            "phpunit-watcher watch"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true,
            "phpro/grumphp-shim": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}